{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:visual": "percy exec -- cypress run --spec 'cypress/e2e/visual-regression.cy.ts'",
    "test:mobile": "cypress run --spec 'cypress/e2e/mobile-testing.cy.ts'",
    "test:browser": "cypress run --spec 'cypress/e2e/cross-browser.cy.ts'",
    "test:load": "k6 run load-tests/performance.js",
    "test:all": "npm run test:run && npm run test:e2e && npm run test:visual",
    "test:ci": "npm run lint && npm run test:run && npm run test:coverage && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "test:ui-consistency": "cypress run --spec 'cypress/e2e/ui-consistency.cy.ts'",
    "test:core-functionality": "cypress run --spec 'cypress/e2e/core-functionality.cy.ts'",
    "test:authentication": "cypress run --spec 'cypress/e2e/authentication.cy.ts'",
    "test:api-integration": "cypress run --spec 'cypress/e2e/api-integration.cy.ts'",
    "test:performance": "cypress run --spec 'cypress/e2e/performance.cy.ts'",
    "test:security": "cypress run --spec 'cypress/e2e/security.cy.ts'",
    "test:dev-tools": "cypress run --spec 'cypress/e2e/dev-tools.cy.ts'",
    "test:polish": "cypress run --spec 'cypress/e2e/polish-final.cy.ts'",
    "test:qa-complete": "npm run test:ui-consistency && npm run test:core-functionality && npm run test:authentication && npm run test:api-integration && npm run test:performance && npm run test:security && npm run test:dev-tools && npm run test:polish",
    "test:lighthouse": "lighthouse http://localhost:5174 --output=json --output-path=./lighthouse-report.json",
    "test:accessibility": "cypress run --spec 'cypress/e2e/accessibility/a11y.test.tsx'",
    "test:cross-browser": "cypress run --spec 'cypress/e2e/cross-browser.cy.ts'",
    "test:mobile-responsive": "cypress run --spec 'cypress/e2e/mobile-testing.cy.ts'",
    "test:performance-optimization": "cypress run --spec 'cypress/e2e/performance.cy.ts'",
    "test:automated": "npm run test:run && npm run test:e2e",
    "test:edge-cases": "cypress run --spec 'cypress/e2e/edge-cases.cy.ts'",
    "test:regression": "cypress run --spec 'cypress/e2e/regression.cy.ts'",
    "test:security-checks": "cypress run --spec 'cypress/e2e/security.cy.ts'",
    "test:production-ready": "npm run test:qa-complete && npm run test:lighthouse && npm run test:load",
    "test:functional": "cypress run --spec 'cypress/e2e/functional-launch.cy.ts'",
    "test:visual-polish": "cypress run --spec 'cypress/e2e/visual-polish.cy.ts'",
    "test:qa-ux": "cypress run --spec 'cypress/e2e/qa-ux-testing.cy.ts'",
    "test:security-access": "cypress run --spec 'cypress/e2e/security-access-control.cy.ts'",
    "test:performance-analytics": "cypress run --spec 'cypress/e2e/performance-analytics.cy.ts'",
    "test:feedback-iteration": "cypress run --spec 'cypress/e2e/feedback-iteration.cy.ts'",
    "test:automation": "cypress run --spec 'cypress/e2e/testing-automation.cy.ts'",
    "test:unit": "vitest run --reporter=verbose",
    "test:utils": "vitest run src/utils/__tests__/",
    "test:hooks": "vitest run src/hooks/__tests__/",
    "test:contexts": "vitest run src/contexts/__tests__/",
    "test:components": "vitest run src/components/__tests__/",
    "test:pr": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:deploy": "npm run build && npm run test:all",
    "test:performance:budget": "lighthouse http://localhost:5173 --budget-path=./lighthouse-budget.json",
    "test:bundle-size": "npm run build && npx webpack-bundle-analyzer dist/stats.json",
    "test:coverage:upload": "codecov",
    "test:browser:chrome": "cypress run --browser chrome",
    "test:browser:firefox": "cypress run --browser firefox",
    "test:browser:safari": "cypress run --browser safari",
    "test:browser:mobile": "cypress run --config-file cypress.mobile.config.ts",
    "test:visual:baseline": "percy exec -- cypress run --spec 'cypress/e2e/visual-regression.cy.ts'",
    "test:load:concurrent": "k6 run load-tests/concurrent-users.js",
    "test:security:audit": "npm audit --audit-level=moderate",
    "test:security:fix": "npm audit fix",
    "test:wcag": "cypress run --spec 'cypress/e2e/accessibility.cy.ts'",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.cache",
    "analyze": "npm run build && npx webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@liveblocks/client": "^3.1.2",
    "@liveblocks/react": "^3.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.2.14",
    "@reduxjs/toolkit": "^2.8.2",
    "@storybook/react": "^9.0.16",
    "@tanstack/react-query": "^5.82.0",
    "@tremor/react": "^3.18.7",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mongoose": "^5.11.96",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode.react": "^1.0.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/speakeasy": "^2.0.10",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.18.1",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.18.2",
    "graphql": "^16.11.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.344.0",
    "mixpanel-browser": "^2.66.0",
    "next": "^15.3.5",
    "openai": "^5.8.3",
    "posthog-js": "^1.257.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-joyride": "^2.7.4",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.22.2",
    "recharts": "^2.15.3",
    "speakeasy": "^2.0.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "@percy/cli": "^1.31.1",
    "@percy/cypress": "^3.1.6",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-themes": "^9.0.16",
    "@storybook/addon-viewport": "^9.0.8",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.9.36",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "cypress": "^14.5.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  }
}
